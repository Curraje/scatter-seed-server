{
  "name": "scatter-seed-server",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "license": "GPL-3.0 License",
  "scripts": {
    "docgen": "openapi-generator-cli generate -i api.yaml -g html2 -0 ./doc",
    "build": "swc --delete-dir-on-start -d build src",
    "build-dev": "cross-env NODE_ENV=development npm run build && node --inspect build/index.js",
    "start": "cross-env NODE_ENV=production npm run build && node build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --ext ts --exec \"npm run build-dev\""
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.145",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/connect-sqlite3": "^0.9.1",
    "@types/express-session": "^1.17.4",
    "@types/graphql": "^14.5.0",
    "@types/node": "^17.0.21",
    "@types/ws": "^8.5.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "prisma": "^3.10.0",
    "ts-node": "^10.5.0",
    "typegraphql-prisma": "^0.19.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.1",
    "connect-sqlite3": "^0.9.13",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "graphql-type-json": "^0.3.2",
    "pg": "^8.7.3",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "tslib": "^2.3.1",
    "type-graphql": "^1.1.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
